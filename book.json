{
	"plugins": [
		"simple-page-toc",
		"anchor-navigation-ex"
	],
	"pluginsConfig": {
		"simple-page-toc": {
			"maxDepth": 3,
			"skipFirstH1": true
		}
	},
	"anchor-navigation-ex": {
		"multipleH1": false,
		"showLevel": false,
		"mode": "float",
		"float": {
			"floatIcon": "fa fa-navicon",
			"showLevelIcon": false,
			"tocLevel1Icon": "fa fa-hand-o-right",
			"tocLevel2Icon": "fa fa-hand-o-right",
			"tocLevel3Icon": "fa fa-hand-o-right"
		},
		"pageTop": {
			"floatIcon": "fa fa-navicon",
			"showLevelIcon": false,
			"tocLevel1Icon": "fa fa-hand-o-right",
			"tocLevel2Icon": "fa fa-hand-o-right",
			"tocLevel3Icon": "fa fa-hand-o-right"
		}
	}
}
